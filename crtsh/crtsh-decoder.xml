<decoder name="crtsh-client">
  <program_name>^crtsh-client</program_name>
</decoder>

<decoder name="crtsh-client">
  <parent>crtsh-client</parent>
  <regex>ID="(\.+)" Logged At="(\.+)" Not Before="(\.+)" Not After="(\.+)" Common Name="(\.+)" Issuer Name="(\.+)" Message="(\.+)"</regex>
  <order>id, loggedAt, notBefore, notAfter, commonName, issuerName, message</order>
</decoder>

